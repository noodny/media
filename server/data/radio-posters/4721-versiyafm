	  <% if (classes.length > 0){ %> 
		class='<%= classes.join(" ") %>'
	  <% } %>
	  <% if (typeof id !== 'undefined') {  %>
		id='<%= id %>' 
	  <% } %>
	  <% if (typeof name !== 'undefined') { %>
		name='<%= name %>' 
	  <% } %>
	  <% if (typeof value !== 'undefined') { %>
		value='<%= value %>' 
