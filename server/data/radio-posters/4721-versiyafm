  <%
	var hasError = ( typeof errors !== 'undefined' && errors[name] );
	var classes = [];

	if(typeof className !== 'undefined' ){
	  classes.push(className);
	}
	  
	if(hasError){
	  classes.push('error');
	}

	%>
	<% if (typeof label !== 'undefined'){ %>
	<label 
	  <% if (typeof id !== 'undefined') { %> 
